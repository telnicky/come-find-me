<h1>Log In</h1>

<%= form_tag sessions_path do %>
  <div class="field">
    <%= label_tag :facebook_id %><br />
    <%= text_field_tag :facebook_id, params[:facebook_id] %>
  </div>
   <div class="field">
    <%= label_tag :facebook_access_token %><br />
    <%= text_field_tag :facebook_access_token, params[:facebook_access_token] %>
  </div>
  <div class="actions"><%= submit_tag "Log In" %>
<% end %>
